// post.jsx
function Post(props) {
  return (
    <div>
      <p>{props.author}</p>
      <p>{props.body}</p>
    </div>
  );
}

export default Post;


// App.jsx
import Post from './components/Post';

function App() {
  return (
    <main>
      <Post author="운전병123" body="직업은 루미" />
      <Post author="삼루트아델" body=" 아델" />
    </main>
  );
}

export default App;
